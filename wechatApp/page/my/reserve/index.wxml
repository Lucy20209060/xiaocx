<view class="reserve-box">

<view class="li" wx:for-items="{{ReserveList}}"  data-id="{{item.id}}" catchtap="gominiReserve">

  <view class="Water-account-box">
    流水号{{item.bookQueueNo}}
    <text class="f_r">{{item.bookQueueNumberStr}}</text>
  </view>

  <view class="type-box">
    <view class="p"><text>餐型</text>{{item.mealTimeStr}}</view>
    <view class="p"><text>类型</text>{{item.deskTypeName}}</view>
    <view class="p"><text>时间</text>{{item.createdDate}}</view>
  </view>

  <view class="number-type-box">
    <text class="f_r">{{item.currentStatusStr}}</text>
  </view>

</view>



</view>