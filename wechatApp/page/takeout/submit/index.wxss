.submit {
  background-color: #f0f0f0;
  height: 100%;
}

page {
  height: 100%;
}

.submit .title {
  height: 100rpx;
  padding: 0 30rpx;
  line-height: 100rpx;
  color: rgb(51, 51, 51);
  font-size: 30rpx;
  border-bottom: 1px solid #dedfe0;
  background: #fff;
}

.list-food .food_li {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  color: rgb(51, 51, 51);
  font-size: 26rpx;
  position: relative;
  background: #fff;
}

.list-food .food_li .t2, .pay-money-box .t2 {
  position: absolute;
  right: 200rpx;
}

.list-food .food_li .t1 {
  color: rgb(142, 142, 142);
}

.pay-money-box {
  height: 80rpx;
  padding: 0 30rpx;
  line-height: 80rpx;
  position: relative;
  background: #fffde2;
  color: rgb(142, 142, 142);
}

.pay-money-box .t3 {
  color: rgb(255, 61, 94);
}

.footer-box {
  position: fixed;
  bottom: 0px;
  left: 0px;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  width: 100%;
}
.footer-box .t1{
  padding-left: 30rpx;
}
.footer-box .t2{
  padding-left: 30rpx;
  color: rgb(255, 61, 94);
}
.footer-box .t3 {
  width: 250rpx;
  background-color: #ff8400;
  text-align: center;
  color: rgb(255, 255, 255);
}
