.take-out-box {
  max-width: 750px;
  min-width: 300px;
  margin: 0 auto;
  /*border: 1px solid blue;*/
}

/*头部*/

.take-out-box .header {
  max-width: 750px;
  min-width: 300px;
  margin: 0 auto;
  width: 100%;
  position: fixed;
  top: 0;
  height: 200rpx;
  background: url("https://lingju360.natappvip.cc/miniapp/resources/images/takeout/banner.png") no-repeat;
  background-size: 100% 100%;
  z-index: 100;
  border-bottom: 20rpx solid #dedfe0;
}

/*左边*/

.take-out-box .header .header-left {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  width: 135rpx;
  height: 135rpx;
  border-radius: 50%;
}

.take-out-box .header .header-left image {
  display: block;
  width: 100%;
}

/*右边*/

.take-out-box .header .header-right {
  height: 100%;
  margin-left: 200rpx;
  color: #fff;
}

.take-out-box .header .header-right text {
  position: relative;
  display: block;
  font-size: 24rpx;
}

.take-out-box .header .header-right text.t1 {
  margin-top: 30rpx;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}

.take-out-box .header .header-right text.t2,
.take-out-box .header .header-right text.t3 {
  line-height: 40rpx;
}

.scroll-main {
  border-top: 10rpx solid #e5e5e5;
}

.scroll-main .zz{
  height: 5000rpx;
  width: 200rpx;
  position: fixed;
  background: #f0f0f0;
  left: 0rem;
}

.scroll-main .zz2{
  height: 10rpx;
  width: 100%;
  position: fixed;
  background: #f0f0f0;
  left: 0rem;
  top: 0rem;
  z-index: 9999;
}

.scroll-main scroll-view {
  max-height: 820rpx;
  position: fixed;
  left: 0px;
  top: 10rpx;
  width: 200rpx;
  float: left;
  background: #f0f0f0;
  z-index: 999999;
}

.scroll-main scroll-view view {
  background-color: #f0f0f0;
  border-bottom: 1px solid #dedfe0;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  padding-left: 60rpx;
  color: #666666;
}
.scroll-main scroll-view view.hot2{
  background: url(https://lingju360.natappvip.cc/miniapp/resources/image/new/z.png) no-repeat;
  background-size:28rpx 26rpx; 
  background-position: 23rpx 38rpx;
}
.scroll-main scroll-view view.hot{
  background: url(https://lingju360.natappvip.cc/miniapp/resources/image/new/r.png) no-repeat;
  background-size:24rpx 26rpx; 
  background-position: 25rpx 38rpx;
}

.scroll-main scroll-view view.active {
  background-color: #fff;
  border-right: 1px solid #fff;
  color: #333;
}

.right-scroll {
  padding-top: 1rpx;
  margin-left: 220rpx;
  padding-bottom: 120rpx;
}

.right-scroll .title {
  border-left: 10rpx solid #ffc834;
  padding-left: 20rpx;
  font-size: 28rpx;
  margin: 20rpx 0 6rpx;
}

.right-scroll .li {
  padding-top: 35rpx;
  height: 160rpx;
  border-bottom: 1px solid #dedfe0;
  position: relative;
}
.right-scroll .li .image_box {
  height: 120rpx;
  width: 120rpx;
  float: left;
  display: block;
  overflow: hidden;
  border-radius: 10rpx;
}
.right-scroll .li image {
  height: 120rpx;
  width: 120rpx;
  float: left;
  display: block;
}

.right-scroll .li .more {
  font-size: 28rpx;
  margin-left: 20rpx;
  float: left;
}
.right-scroll .li .more view.t1,.max_box .more view.t1{
  color: #666;
  font-size: 30rpx;
  font-weight: 800;
}

.right-scroll .li .more .t3 view {
  font-size: 34rpx;
  float: left;
}
.right-scroll .li .more .t2{
  padding: 8rpx 0;
}
.right-scroll .li .more .t2, .right-scroll .li .more .t3 {
  font-size: 24rpx;
  color: #999;
}
.right-scroll .li .more .t3 {
  color: #ff8400;
}
.right-scroll .li .more .t3 .n {
  font-size: 24rpx;
  transform: translateY(10rpx);
}

.right-scroll .li .jia,.yes-select-menu-box .ul_menu .li_menu .jia {
position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.right-scroll .li .imgbox{
  
  left: 20rpx;
}
.right-scroll .li .jian,.yes-select-menu-box .ul_menu .li_menu .jian {
  right: 120rpx;
}

.right-scroll .li .num,.yes-select-menu-box .ul_menu .li_menu .num {
  right: 72rpx;
  line-height: 50rpx;
  text-align: center;
}

.footer {
  position: fixed;
  bottom: -1rpx;
  height: 100rpx;
  width: 100%;
  left: 0;
  background: #fff;
  border-top: 1rpx solid #f0f0f0;
  z-index: 9999999999999999999;
}

.footer .rmb {
  color: #ff8400;
  line-height: 100rpx;
  padding-left: 100rpx;
  float: left;
}

.footer .sub {
  top: 0px;
  position: absolute;
  right: 0px;
  margin: 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  width: 200rpx;
  background-color: #ff8400;
  font-size:30rpx;
  border-radius: 8rpx;
}

.footer .sub.no{
  background: #f0f0f0;
}

.footer .num {
  position: absolute;
  background: #fff;
  width: 60rpx;
  height: 60rpx;
  left: 20rpx;
  top: 20rpx;
  z-index: 9999999999;
  text-align: center;
  line-height: 56rpx;
  font-size: 30rpx;
  color: #ff8400;
  border-radius: 50%;
  border:2rpx solid #ff8400;
  box-sizing: border-box;
}

.footer image {
  margin: 20rpx;
  width: 60rpx;
  height: 60rpx;
}
.yes-select-menu-box .input_box{
  border-top: 1rpx solid #f0f0f0;
  padding: 30rpx;
  background: #fff;
  border-radius:0 0 8rpx 8rpx; 
}

.yes-select-menu-box .input_box input{
  background: #f0f0f0;
  height: 80rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
  color: #999;
}
.yes-select-menu-box {
  box-sizing: border-box;
  padding: 20rpx;
  position: fixed;
  bottom: -454rpx;
  max-width: 750px;
  min-width: 300px;
  margin: 0 auto 0 auto;
  width: 100%;
  height: 540rpx;
  background-color: #f0f0f0;
  overflow: hidden;
  border-top: 1px solid #eee;
  transition: all 0.3s ease;
  z-index: 999999999;
}

.yes-select-menu-box .yes-select-menu {
  padding: 0 30rpx;
  height: 80rpx;
  color: #333;
  background: #fff;
  border-radius: 8rpx 8rpx 0 0;
  border-bottom: 1rpx solid #f0f0f0;
}
.yes-select-menu-box .yes-select-menu  .cl{
  font-size: 28rpx;
  position: relative;
  padding-left: 50rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.yes-select-menu-box .yes-select-menu  .cl image{
  position: absolute;
  left: 0rpx;
  top: 20rpx;
  width: 40rpx;
  height: 40rpx;
}
.yes-select-menu-box .yes-select-menu text {
  display: block;
  line-height: 80rpx;
}

.mask-layer-box {
  position: fixed;
  top: 0;
  max-width: 750px;
  min-width: 300px;
  margin: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  display: block;
  z-index: 999999999;
  
}

.yes-select-menu-box .ul_menu {
  background: #fff;
  height: 300rpx;
  padding:0 30rpx;
  width: auto;
  z-index: 999999999999999;
}

.yes-select-menu-box .ul_menu .li_menu{
  height: 100rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  position: relative;
  border-bottom: 1px solid #f0f0f0;
}

.yes-select-menu-box .ul_menu .li_menu text.f_r{
  margin-right: 200rpx;
  color: #ff2d51;
}
.yes-select-menu-box .ul_menu .li_menu .jia{
  bottom: 25rpx;
}

.ani{
  bottom:100rpx;
}

.zzzz{
  z-index: 9999999999999;
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
}
.max_box{
  position: fixed;
  z-index: 999999999999999;
  top: 50%;
  left: 50%;
  margin-left: -300rpx;
  margin-top: -1300rpx;
  border-radius: 20rpx;
  width: 600rpx;
  height: 650rpx;
  overflow: hidden;
  background: #fff;
  transition: all 0.3s ease;
  opacity: 0;
}

.max_box.anni{
  opacity: 1;
  margin-top: -500rpx;
}

.max_box .img_box{
  width: 100%;
  height: 450rpx;
  overflow: hidden;
}

.max_box .img_box image{
  width: 100%;
}

.max_box .more{
  background: #fff;
}

.max_box .more {
  padding: 30rpx 0;
  font-size: 28rpx;
  margin-left: 20rpx;
  float: left;
}
.max_box .more view.t1,.max_box .more view.t1{
  color: #666;
  font-size: 32rpx;
  font-weight: 800;
}

.max_box .more .t3 view {
  font-size: 34rpx;
  float: left;
}
.max_box .more .t2{
  padding: 12rpx 0;
}
.max_box .more .t2, .max_box .more .t3 {
  font-size: 24rpx;
  color: #999;
}
.max_box .more .t3 {
  color: #ff8400;
}


.max_box .jia,.yes-select-menu-box .ul_menu .li_menu .jia {
position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.max_box .imgbox{
  
  left: 20rpx;
}
.max_box .jian,.yes-select-menu-box .ul_menu .li_menu .jian {
  right: 120rpx;
}

.max_box .num,.yes-select-menu-box .ul_menu .li_menu .num {
  right: 72rpx;
  line-height: 50rpx;
  text-align: center;
}