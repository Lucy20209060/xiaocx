.toLogin{
  position: fixed;
  z-index: 999999999;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
page {
  width: 100%;
  font-family: '微软雅黑';
  height: 100%;
}

.display_none{
  display: none;
}

.display_block{
  display: block;
}

.f_l{
  float: left;
}
.f_r{
  float: right;
}
text {
  font-size: 30rpx;
}

/* zhuye */

/* page/vichily/index.wxss */

.header {
  overflow: hidden;
}

.imgsss {
  width: 100%;
  display: block;
}

.main {
  padding-top: 15rpx;
  height: 130rpx;
  border-top: 20rpx solid #f0f0f0;
  border-bottom: 20rpx solid #f0f0f0;
  position: relative;
  background: #fff;
}

.main text {
  display: block;
  padding-left: 60rpx;
  line-height: 60rpx;
  position: relative;
  color: #333333;
  font-size: 28rpx;
  overflow: hidden;
  height: 60rpx;
  max-width: 500rpx;

}

.main text.t1::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.natappvip.cc/miniapp/resources/image/icon/4.png') no-repeat ;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main text.t2::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.natappvip.cc/miniapp/resources/image/icon/1.png') no-repeat;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main .phoneRight {
  position: absolute;
  right: 0px;
  top: 0px;
  border-left: 1px solid #f0f0f0;
  height: 150rpx;
  width: 170rpx;
  text-align: center;
}

.main .phoneRight .icon2_phone {
  display: inline-block;
  margin-top: 40rpx;
  padding: 0px;
  height: 56rpx;
  width: 56rpx;
  background: url('https://lingju360.natappvip.cc/miniapp/resources/image/icon/p.png') no-repeat;
  background-size: 56rpx 56rpx;
}

.twoSection .title {
  text-align: center;
  border-bottom: 1px solid #ffc834;
  padding: 15rpx 0;
  color: #ffc834;
  font-size: 40rpx;
}

.twoSection .lj_firstDiv {
  margin: 60rpx 60rpx 20rpx;
  border: 1px solid #f0f0f0;
}

.twoSection .lj_firstDiv .table-title {
  padding: 20rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #f0f0f0;
}

.twoSection .lj_firstDiv .table-title text {
  color: #ffc834;
}

.twoSection .lj_firstDiv .li-3 {
  text-align: center;
  overflow: hidden;
}

.twoSection .lj_firstDiv .li-3 .li {
  float: left;
  width: 33%;
  font-size: 40rpx;
}

.twoSection .lj_firstDiv .li-3 .li .t {
  display: inline-block;
  font-size: 35rpx;
  margin: 20rpx 0 0;
}

.twoSection .lj_firstDiv .li-3 .li .more {
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #8e8e8e;
}

.twoSection .lj_firstDiv .li-3 .li .more text {
  color: #ffc834;
  font-size: 40rpx;
  margin-right: 10rpx;
}

.twoSection .ts {
  display: inline-block;
  padding: 0 60rpx;
  color: #8e8e8e;
  margin:30rpx 0 30rpx;
  position: relative;
}

.twoSection .ts::after {
  content: '';
  position: absolute;
  left: 10px;
  top: 7rpx;
  padding: 0px;
  height: 30rpx;
  width: 30rpx;
  background: url('https://lingju360.natappvip.cc/miniapp/resources/image/icon/7.png') no-repeat;
  background-size: 30rpx 30rpx;
}

.btn {
  background: #ff8400;
  color: #fff;
  border-radius: 60rpx;
  margin: 0 40rpx;
  height: 80rpx;
  line-height: 80rpx;
}

input[type="button"], input[type="submit"], input[type="reset"] {
  -webkit-appearance: none;
}

textarea {
  -webkit-appearance: none;
}
button::after {
  border: 0;
} 

.ul{
  overflow: hidden;
  border-bottom: 20rpx solid #f0f0f0;
}
.ul .li{
  width: 25%;
  float: left;
  text-align: center;
}

.ul .li image{
  display: block;
  width: 52%;
  margin: 0 auto;
}

.ul .li text{
  display: block;
  font-size: 28rpx;
  color: #333333;
  margin: 20rpx 0 40rpx ;
}

.btn_bom{
  position: fixed;
  bottom: 0;
  left: 0;
  height: 124rpx;
  border-top: 1rpx solid #c8c8c8;
  width: 100%;text-align: center;
  padding: 26rpx;
  box-sizing: border-box;
  background: #fff;
}

.btn_bom view{
  background-color: #ff8400;
  height: 72rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 8rpx;
  line-height: 72rpx;
}


.table{
  background: #fff;
  margin-top: 30rpx;
  margin-bottom: 100rpx;
}

.table .hander,.table .body{
  color: #999;
  text-align: center;
}

.table  text{
  line-height: 90rpx;
  display: inline-block;
}

.table .li text:nth-of-type(1){
  width: 175rpx;
}

.table .li text:nth-of-type(2){
  width: 150rpx;
}

.table .li text:nth-of-type(3){
  width: 150rpx;
}

.table .li text:nth-of-type(4){
  width: 250rpx;
}

.table .body{
  color: #333;
  border-top: 1rpx solid #f0f0f0;
}

.table .body.li text:nth-of-type(1){
  text-align: center;
}

.table .body.li text:nth-of-type(2){
  color: #999;
  
}


.table .li{
  height: 90rpx;
  overflow: hidden;
}

.table .li view{
  width: 250rpx;
  display: inline-block;
  text-align: center;
  line-height: 90rpx;
  position: relative;
  top: 11rpx;
}

.table .li view image{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
}

.table .li view text{
  width: 60rpx !important;
  height: 50rpx;
  position: relative;
  top: -10rpx;
  display: inline-block;
}

.table .foot{
  border-top: 1rpx solid #f0f0f0;
  color: #666;
  font-size: 30rpx;
  text-align: right;
  padding-right: 70rpx;
}

.table .foot text{
  color: #ff3d5e;
}